<?xml version="1.0" encoding="utf-8"?>
<widget xmlns="http://www.mendix.com/widget/1.0/"
        id="Lync.widget.Lync"
        needsEntityContext="true">
    <name>Lync</name>
    <description>Ability to click a link and launch a Lync conversation</description>
    <icon></icon>
    <properties>
        <property key="email" type="attribute">
            <caption>Email Address</caption>
            <category>Contact Details</category>
            <description>Email Address</description>
            <attributeTypes>
                <attributeType name="String"/>
            </attributeTypes>
        </property>
        <property key="displaytext" type="attribute">
            <caption>Display Text</caption>
            <category>Contact Details</category>
            <description>The display text the user will see</description>
            <attributeTypes>
                <attributeType name="String"/>
            </attributeTypes>
        </property>
    </properties>
</widget>